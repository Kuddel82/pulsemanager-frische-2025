{
  "version": 2,
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm ci",
  "devCommand": "npm run dev",
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ],
  "env": {
    "BUILD_ID": "runtime-errors-fixed-2025-01-14"
  },
  "build": {
    "env": {
      "NODE_ENV": "production",
      "FORCE_REBUILD": "true"
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}